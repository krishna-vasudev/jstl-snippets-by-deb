{
  "name": "jstl-snippets-by-deb",
  "displayName": "JSTL snippets by deb",
  "description": "This extension generates JSTL snippets for .jsp although the language mode is HTML in vscode.",
  "icon": "images/jstl-snippets-by-deb-logo.png",
  "version": "0.0.2",
  "publisher": "DebrajBhal",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "repository": {
        "type": "git",
        "url": "https://github.com/krishna-vasudev/jstl-snippets-by-deb.git"
    }
}
